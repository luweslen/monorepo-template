{
  "name": "monorepo-template",
  "version": "1.0.1",
  "description": "Monorepo template using pnpm workspaces, NestJS and VueJS",
  "type": "module",
  "scripts": {
    "server:dev": "pnpm --filter server dev",
    "server:build": "pnpm --filter server build",
    "server:start": "pnpm --filter server start",
    "server:test:unit": "pnpm --filter server test:unit",
    "server:test:e2e": "pnpm --filter server test:e2e",
    "server:test:ci": "pnpm --filter server test:ci",
    "client:dev": "pnpm --filter client dev",
    "client:build": "pnpm --filter client build",
    "client:preview": "pnpm --filter client preview",
    "client:test:unit": "pnpm --filter client test:unit",
    "client:test:e2e": "pnpm --filter client test:e2e",
    "lint": "pnpm --filter \"./...\" lint"
  },
  "keywords": [
    "monorepo",
    "pnpm",
    "nestjs",
    "vuejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/luweslen/monorepo-template.git"
  },
  "author": {
    "name": "Luciano Weslen",
    "url": "https://github.com/luweslen"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.15.0",
    "pnpm": ">=10.25.0"
  },
  "packageManager": "pnpm@10.25.0"
}